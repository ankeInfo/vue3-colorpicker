{
  "name": "@ankeinfo/vue3-colorpicker",
  "version": "2.3.2",
  "main": "dist/index.es.js",
  "types": "dist/types/index.d.ts",
  "author": "aesoper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ankeInfo/vue3-colorpicker/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/ankeInfo/vue3-colorpicker#readme",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && vue-tsc --noEmit && vite build",
    "build:lib": "rimraf dist && vite build --config ./vite.config.lib.ts",
    "serve": "vite preview",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "prepare": "husky install",
    "clean:dist": "rimraf dist",
    "pub": "cd dist && npm publish --registry=https://registry.npmjs.org --access public && cd ..",
    "unpub": "npm unpublish @ankeinfo/vue3-colorpicker --force --registry=https://registry.npmjs.org",
    "prelink": "cd dist && yarn link && cd .. && yarn link  @ankeinfo/vue3-colorpicker",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "storybook dev",
    "build-storybook": "rimraf storybook-static && storybook build",
    "reinstall": "rimraf node_modules && yarn install"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vueuse/core": "^13.9.0",
    "gradient-parser": "^1.1.1",
    "lodash-es": "^4.17.21",
    "tinycolor2": "^1.6.0",
    "vue": "^3.5.22",
    "vue-types": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@storybook/addon-actions": "^ 9.0.8",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^9.1.8",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/cli": "^9.1.8",
    "@storybook/vue3": "^9.1.8",
    "@storybook/vue3-vite": "^9.1.8",
    "@types/gradient-parser": "^1.1.0",
    "@types/jest": "^30.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.5.2",
    "@types/tinycolor2": "^1.4.6",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/compiler-sfc": "^3.5.22",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0 ",
    "babel-loader": "^10.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-storybook": "^9.1.8",
    "eslint-plugin-vue": "^10.5.0",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.2.1",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^3.6.2",
    "pretty-quick": "^4.2.2",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "storybook": "^8.6.14",
    "stylelint": "^16.24.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vite-plugin-dts": "^4.5.4",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^3.0.8"
  }
}
